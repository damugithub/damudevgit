					"source": {
						"type": "OracleSource",
						"oracleReaderQuery": "/*CT_ORG_D_STG*/\r\nSELECT COALESCE (HU.ORGANIZATION_ID,0)  AS ORG_ID,\r\n  HU.NAME                  AS ORG_NAME,\r\n  NULL                     AS ORG_UNIT_CLASSIFICATION_ID,\r\n  HU.ORGANIZATION_ID       AS ORG_NUM,\r\n  HRL.DESCRIPTION          AS ORG_DESC,\r\n  NULL                     AS CLASSIFICATION_CODE,\r\n  NULL                     AS CLASSIFICATION_NAME,\r\n  HRL.ADDRESS_LINE_1       AS ST_ADDRESS1,\r\n  HRL.ADDRESS_LINE_2       AS ST_ADDRESS2 ,\r\n  HRL.TOWN_OR_CITY         AS CITY,\r\n  '-'                      AS COUNTY,\r\n  HRL.POSTAL_CODE          AS POSTAL_CODE,\r\n  HRL.REGION_2             AS STATE_CODE,\r\n  S.MEANING                AS STATE_NAME,\r\n  HRL.REGION_1             AS STATE_REGION,\r\n  HRL.COUNTRY              AS COUNTRY_CODE,\r\n  FND.TERRITORY_SHORT_NAME AS COUNTRY_NAME,\r\n  '-'                      AS COUNTRY_REGION,\r\n  '-'                      AS WEB_ADDRESS,\r\n  HRL.TELEPHONE_NUMBER_1   AS PHONE_NUM,\r\n  '-'                      AS EMAIL_ADDRESS,\r\n  '-'                      AS DIVN_FLAG,\r\n  '-'                      AS BU_FLAG,\r\n  (\r\n  CASE\r\n    WHEN HI.ORG_INFORMATION1='OPERATING_UNIT'\r\n    THEN 'Y'\r\n    ELSE 'N'\r\n  END) OPERATING_UNIT_FLAG,\r\n  (\r\n  CASE\r\n    WHEN HI.ORG_INFORMATION1='INV'\r\n    THEN 'Y'\r\n    ELSE 'N'\r\n  END) INV_ORG_FLAG,\r\n  (\r\n  CASE\r\n    WHEN HI.ORG_INFORMATION1 = 'HR_ORG'\r\n    THEN 'Y'\r\n    ELSE 'N'\r\n  END) HR_ORG_FLAG,\r\n  (\r\n  CASE\r\n    WHEN HI.ORG_INFORMATION1 = 'PA_PROJECT_ORG'\r\n    THEN 'Y'\r\n    ELSE 'N'\r\n  END) PROJECT_ORG_FLAG,\r\n  '-'      AS LEGAL_ENTITY         ,\r\n  '-'      AS STATUS               ,\r\n  '-'      AS COST_CENTER_CODE     ,\r\n  TO_TIMESTAMP(HU.DATE_FROM) AS EFFECTIVE_START_DT            ,\r\n  TO_TIMESTAMP(HU.DATE_TO )  AS EFFECTIVE_END_DT              ,\r\n  CASE WHEN  SYSDATE BETWEEN HU.DATE_FROM and NVL(HU.DATE_TO,SYSDATE) THEN 'Y' else 'N' END AS CURRENT_FLAG,\r\n  HU.ATTRIBUTE1     AS ATTRIBUTE1  ,\r\n  HU.ATTRIBUTE2     AS ATTRIBUTE2  ,\r\n  HU.ATTRIBUTE3     AS ATTRIBUTE3  ,\r\n  HU.ATTRIBUTE4     AS ATTRIBUTE4  ,\r\n  HU.ATTRIBUTE5     AS ATTRIBUTE5  ,\r\n  TO_TIMESTAMP(HI.LAST_UPDATE_DATE)         AS LAST_UPDATE_DT,\r\n  TO_TIMESTAMP(HI.CREATION_DATE)            AS CREATION_DT,\r\n  FU_CREATION.USER_NAME       AS CREATED_BY ,\r\n  FU_LASTUPDATE.USER_NAME     AS LAST_UPDATED_BY,\r\n  HI.ORG_INFORMATION1||'~'||TO_CHAR(HU.ORGANIZATION_ID) AS INTEGRATION_ID,\r\n  1000                        AS DATASOURCE_NUM_ID\r\nFROM \r\n  HR_ALL_ORGANIZATION_UNITS HU,\r\n  HR_ORGANIZATION_INFORMATION HI,\r\n  HR_LOCATIONS_ALL HRL,\r\n  MTL_PARAMETERS ORG_PARM,\r\n  FND_TERRITORIES_TL FND,\r\n  FND_USER FU_CREATION,\r\n  FND_USER FU_LASTUPDATE,\r\n  FND_LOOKUP_VALUES S\r\nWHERE HI.ORG_INFORMATION_CONTEXT = 'CLASS'\r\nAND HI.ORG_INFORMATION1          IN ('INV','HR_ORG','PA_PROJECT_ORG','OPERATING_UNIT' )\r\nAND HI.ORG_INFORMATION2           = 'Y'\r\nAND HI.ORGANIZATION_ID            = HU.ORGANIZATION_ID\r\nAND HU.LOCATION_ID                = HRL.LOCATION_ID (+)\r\nAND HU.ORGANIZATION_ID            = ORG_PARM.ORGANIZATION_ID (+)\r\nAND FND.TERRITORY_CODE            =HRL.COUNTRY\r\nAND FU_CREATION.USER_ID           = HI.CREATED_BY\r\nAND FU_LASTUPDATE.USER_ID         = HI.LAST_UPDATED_BY\r\nAND S.LOOKUP_TYPE (+)                = 'STATE'\r\nAND S.LOOKUP_CODE(+)                 = HRL.REGION_2\r\nAND (START_DATE_ACTIVE            IN\r\n  (SELECT MAX(START_DATE_ACTIVE)\r\n  FROM FND_LOOKUP_VALUES FLV\r\n  WHERE FLV.LOOKUP_CODE= S.LOOKUP_CODE\r\n  AND FLV.LOOKUP_TYPE  = S.LOOKUP_TYPE\r\n  GROUP BY LOOKUP_CODE,\r\n    LOOKUP_TYPE\r\n  ) OR START_DATE_ACTIVE IS NULL)\r\n--AND TO_CHAR(HI.LAST_UPDATE_DATE, 'YYYY-MM-DD HH24:MI:SS') >= '2022-01-01'",
