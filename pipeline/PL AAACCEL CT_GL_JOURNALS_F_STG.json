					"source": {
						"type": "OracleSource",
						"oracleReaderQuery": "/*CT_GL_JOURNALS_F_STG*/\r\nSELECT\r\n\tTO_CHAR(L.LEDGER_ID) as LEDGER_ID,\r\n\tTO_CHAR(GCC.CODE_COMBINATION_ID) as GLCC_ID,\r\n\tTO_NUMBER(TO_CHAR(L.EFFECTIVE_DATE,'YYYYMMDD')) as PERIOD_DT,\r\n\tTO_NUMBER(TO_CHAR(L.EFFECTIVE_DATE,'YYYYMMDD')) as ACCOUNTING_DT,\r\n\tTO_NUMBER(TO_CHAR(H.POSTED_DATE,'YYYYMMDD')) as POSTED_DT,\r\n\tGL.NAME as LEDGER_NAME, \r\n\tL.PERIOD_NAME as PERIOD_NAME,\r\n\tGP.PERIOD_YEAR as PERIOD_YEAR,\r\n\tGP.PERIOD_NUM as PERIOD_NUM,\r\n\tGCC.SEGMENT1 as SEGMENT1,\r\n\tGCC.SEGMENT2 as SEGMENT2,\r\n\tGCC.SEGMENT3 as SEGMENT3,\r\n\tGCC.SEGMENT4 as SEGMENT4,\r\n\tGCC.SEGMENT5 as SEGMENT5,\r\n\tGCC.SEGMENT6 as SEGMENT6,\r\n\tGCC.SEGMENT7 as SEGMENT7,\r\n\tGCC.SEGMENT8 as SEGMENT8,\r\n\tGCC.SEGMENT9 as SEGMENT9,\r\n\tGL.CURRENCY_CODE as LEDGER_CURR_CODE, -- Mapped with correct column\r\n\tH.CURRENCY_CODE as TRANS_CURR_CODE, -- NEED TO VERIFY\r\n\tB.NAME as JE_BATCH_NAME,\r\n\tB.STATUS as JE_BATCH_STATUS,\r\n\tL.DESCRIPTION as JE_BATCH_DESC,\r\n\tH.JE_SOURCE as JE_BATCH_SOURCE,\r\n\tH.PERIOD_NAME as JE_BATCH_PERIOD_NAME,\r\n\tH.POSTED_DATE as JE_BATCH_POSTED_DT,\r\n\tB.DEFAULT_EFFECTIVE_DATE as JE_BATCH_ACCOUNTING_DT,\r\n\tB.APPROVAL_STATUS_CODE as JE_BATCH_APPROVAL_STATUS,\r\n\tL.JE_HEADER_ID as JE_HEADER_ID,\r\n\tL.JE_LINE_NUM as JE_LINE_NUM,\r\n\tH.NAME as JE_HEADER_NAME, \r\n\tH.DESCRIPTION as JE_HEADER_DESCRIPTION, -- Mapped with correct column\r\n\tH.JE_SOURCE as JE_HDR_SOURCE,\r\n\tH.JE_CATEGORY as JE_HDR_CATEGORY,\r\n\tH.EXTERNAL_REFERENCE as JE_HDR_SOURCE_REF, -- Mapped with correct column\r\n\tNULL as JE_HDR_REFERENCE,\r\n\tH.STATUS as JE_HDR_STATUS,\r\n\tH.PERIOD_NAME as JE_HDR_PERIOD_NAME,\r\n\tH.POSTED_DATE as JE_HDR_POSTED_DT,\r\n\tH.DEFAULT_EFFECTIVE_DATE as JE_HDR_ACCOUNTING_DT,\r\n\tH.ACTUAL_FLAG as ACTUAL_FLAG,\r\n\tB.STATUS_VERIFIED as STATUS_VERIFIED,\r\n\tL.STATUS as JE_LINE_STATUS, \r\n\tL.DESCRIPTION as JE_LINE_DESCRIPTION, \r\n\tH.PERIOD_NAME as JE_LINE_PERIOD_NAME,\r\n\tL.EFFECTIVE_DATE as JE_LINE_ACCOUNTING_DT,\r\n\tNULL as JE_LINE_CURRENCY_CODE, \r\n\tH.CURRENCY_CONVERSION_RATE as JE_LINE_CONV_RATE,\r\n\tH.CURRENCY_CONVERSION_TYPE as JE_LINE_CONV_TYPE, \r\n\tH.CURRENCY_CONVERSION_DATE as JE_LINE_CONV_DT, \r\n\tL.ENTERED_DR as ENTERED_DR,\r\n\tL.ENTERED_CR as ENTERED_CR,\r\n\tNVL(L.ACCOUNTED_DR,0) as ACCOUNTED_DR,\r\n\tNVL(L.ACCOUNTED_CR,0) as ACCOUNTED_CR,\r\n\tNVL(L.ENTERED_DR,0)*NVL(R.CONVERSION_RATE,1) as ENTERED_DR_USD, -- Updated with conversion logic\r\n\tNVL(L.ENTERED_CR,0)*NVL(R.CONVERSION_RATE,1) as ENTERED_CR_USD, -- Updated with conversion logic\r\n\tNVL(L.ACCOUNTED_DR,0)*NVL(R.CONVERSION_RATE,1) as ACCOUNTED_DR_USD,\r\n\tNVL(L.ACCOUNTED_CR,0)*NVL(R.CONVERSION_RATE,1) as ACCOUNTED_CR_USD,\r\n\tR.CONVERSION_RATE as CONV_RATE_TO_USD,\r\n\tR.CONVERSION_TYPE as CONV_RATE_TYPE,\r\n\tB.ATTRIBUTE1 as ATTRIBUTE1,\r\n\tB.ATTRIBUTE2 as ATTRIBUTE2,\r\n\tB.ATTRIBUTE3 as ATTRIBUTE3,\r\n\tB.ATTRIBUTE4 as ATTRIBUTE4,\r\n\tB.ATTRIBUTE5 as ATTRIBUTE5,\r\n\tB.ATTRIBUTE6 as ATTRIBUTE6,\r\n\tB.ATTRIBUTE7 as ATTRIBUTE7,\r\n\tB.ATTRIBUTE8 as ATTRIBUTE8,\r\n\tB.ATTRIBUTE9 as ATTRIBUTE9,\r\n\tB.ATTRIBUTE10 as ATTRIBUTE10,\r\n\tL.CREATION_DATE as CREATION_DT,\r\n\tL.LAST_UPDATE_DATE as LAST_UPDATE_DT,\r\n\tL.CREATED_BY as CREATED_BY,\r\n\tL.LAST_UPDATED_BY as LAST_UPDATED_BY,\r\n\tL.JE_HEADER_ID|| '~'|| L.JE_LINE_NUM as INTEGRATION_ID,\r\n\t1000 as DATASOURCE_NUM_ID,\r\n\t'Y' as ACTIVE_FLAG,\r\n\t'N' as DELETE_FLAG,\r\n\tCURRENT_DATE as W_INSERT_DT,\r\n\tCURRENT_DATE as W_UPDATE_DT\r\nFROM\r\n\tGL_JE_LINES L,\r\n\tGL_JE_HEADERS H,\r\n\tGL_JE_BATCHES B,\r\n    GL_LEDGERS GL,\r\n\tFND_USER FU,\r\n\tGL_CODE_COMBINATIONS GCC,\r\n\tGL_PERIODS GP,\r\n    GL_DAILY_RATES R\r\nWHERE 1=1\r\n\tAND L.JE_HEADER_ID = H.JE_HEADER_ID\r\n\tAND H.JE_BATCH_ID = B.JE_BATCH_ID\r\n\tAND L.CREATED_BY = FU.USER_ID(+)\r\n\tAND GCC.CODE_COMBINATION_ID = L.CODE_COMBINATION_ID\r\n\tAND GL.LEDGER_ID = H.LEDGER_ID\r\n\tAND L.PERIOD_NAME = GP.PERIOD_NAME\r\n\tAND GP.PERIOD_SET_NAME = B.PERIOD_SET_NAME\r\n\tAND R.FROM_CURRENCY(+) = H.CURRENCY_CODE\r\n\tAND R.TO_CURRENCY(+) = 'USD'\r\n\tAND R.CONVERSION_TYPE(+) = 'Corporate'\r\n\tAND TRUNC(R.CONVERSION_DATE(+)) = TRUNC(H.POSTED_DATE)\r\n\t--AND TO_CHAR(L.LAST_UPDATE_DATE, 'YYYY-MM-DD HH24:MI:SS') >= '2022-01-01'\r\n;",
						"partitionOption": "None"
					},
